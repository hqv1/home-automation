{
  "auditing": {
    "audit-on-successful-event": true,
    "detail-audit-on-successful-event": false
  },
  "logging": {
    "minimum-level": "Information",
    "path": "log/logs.json"
  },
  "ecobee": {
    "base-uri": "https://api.ecobee.com",
    "token-uri": "token",
    "thermostat-uri": "1/thermostat",
    "should-log-result": "false"
  },
  "redis": {
    "auth-token-timeout-in-secs": 60
  }
}